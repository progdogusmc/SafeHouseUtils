<Window x:Class="Restart_Voicemail.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Restart_Voicemail"
        mc:Ignorable="d"
        Title="Restarting Voicemail..." Height="150" Width="500" Icon="restart_voicemail.ico" Loaded="Window_Loaded" Closed="Window_Closed"> 
    <Grid>
        <TextBlock Name="StatusTextBlock" Margin="0" VerticalAlignment="Center" HorizontalAlignment="Stretch" TextAlignment="Center" FontSize="24" FontWeight="DemiBold">Voicemail restarting...</TextBlock>
    </Grid>
</Window>
